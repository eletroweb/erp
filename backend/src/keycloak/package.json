  "dependencies": {
    "reflect-metadata": "^0.2.1",
    "@nestjs/common": "^10.3.3",
    "@nestjs/core": "^10.3.3",
    "keycloak-connect": "^23.0.6",
    "request-promise": "^4.2.6",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "cpr": "^3.0.1",
    "prettier": "^3.2.5",
    "reflect-metadata": "^0.2.1",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  }
